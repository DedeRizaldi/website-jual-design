{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2022",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": false,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "outDir": "./dist",
        "rootDir": ".",
        "types": [
            "node"
        ]
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}